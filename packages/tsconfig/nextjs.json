{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "ignoreDeprecations": "5.0",
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true,
    "noImplicitAny": false,
    "strict": false
  }
}
